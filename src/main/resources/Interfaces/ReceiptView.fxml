<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="730.0" prefWidth="337.0" stylesheets="@/stylesheet.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.ReceiptController">
   <children>
      <BorderPane prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <center>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
               <children>
                  <Text fill="#66310e" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="text" text="Order">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Text>
                  <Text fx:id="orderNumberText" fill="#66310e" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="text" text="7">
                     <font>
                        <Font size="120" />
                     </font>
                  </Text>
               </children>
            </VBox>
         </center>
         <bottom>
            <Button fx:id="exitButton" mnemonicParsing="false" onAction="#exitOrder" styleClass="buttonSimpleStyle" text="Exit" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets bottom="20.0" />
               </BorderPane.margin></Button>
         </bottom>
      </BorderPane>
   </children>
</AnchorPane>
